
public class Iphone {

	public static void main(String[] args) {
		smartPhone iphone = new smartPhone();
		iphone.play();
		iphone.stop();
		iphone.next();
		iphone.back();
		iphone.call();
		iphone.mail();
		iphone.photo();
		iphone.internet();

	}

	public static void main(String[] args) {
		smartPhone Android = new smartPhone();
		Android.play();
		Android.stop();
		Android.next();
		Android.back();
		Android.call();
		Android.mail();
		Android.photo();
		Android.internet();

	}
}
